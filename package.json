{
  "name": "realar",
  "version": "0.3.6",
  "description": "React state manager",
  "repository": {
    "url": "https://github.com/betula/realar"
  },
  "bugs": {
    "url": "https://github.com/betula/realar/issues"
  },
  "homepage": "https://github.com/betula/realar#readme",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "jest",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "jest",
    "publish": "npm run clear && npm run build && lerna publish",
    "format": "prettier --write src tests jest",
    "clear": "rimraf build"
  },
  "dependencies": {
    "reactive-box": ">=0.5.0"
  },
  "optionalDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.19",
    "@types/react": "17.0.0",
    "babel-jest": "26.6.3",
    "babel-plugin-realar": "0.2.1",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rimraf": "3.0.2",
    "typescript": "4.1.3"
  },
  "author": "Slava Birch <mail@betula.co> (http://betula.co)",
  "keywords": [
    "state",
    "model",
    "reactive",
    "observable",
    "frontend",
    "react",
    "state management",
    "state manager",
    "data flow",
    "react hooks",
    "reactive programming",
    "typescript",
    "javascript",
    "jsx",
    "decorator"
  ],
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false
  }
}
